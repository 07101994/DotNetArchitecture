{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"project": { "name": "UI" },
	"apps": [
		{
			"environmentSource": "environments/environment.ts",
			"environments": {
				"development": "environments/environment.ts",
				"production": "environments/environment.production.ts",
				"staging": "environments/environment.staging.ts"
			},
			"index": "index.html",
			"main": "main.ts",
			"outDir": "dist",
			"polyfills": "polyfills.ts",
			"prefix": "app",
			"root": "src",
			"styles": [ "styles.scss" ],
			"tsconfig": "tsconfig.app.json"
		}
	],
	"defaults": { "build": { "progress": true }, "styleExt": "scss" },
	"lint": [ { "files": "src/**/*.ts", "project": "tsconfig.json" } ],
	"warnings": { "typescriptMismatch": false }
}
