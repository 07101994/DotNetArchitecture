{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "UI",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}/Web/UI/bin/Debug/netcoreapp2.0/Solution.Web.UI.dll",
			"cwd": "${workspaceFolder}/Web/UI",
			"stopAtEntry": false,
			"internalConsoleOptions": "openOnSessionStart",
			"launchBrowser": {
				"enabled": true,
				"args": "${auto-detect-url}",
				"windows": {
					"command": "cmd.exe",
					"args": "/C start ${auto-detect-url}"
				}
			},
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			}
		}
	]
}
